{
	"name": "<%= projectName.toLowerCase() %>",
	"version": "1.0.0",
	"description": "The build tool for Front-end using Gulp4",
	"main": "index.js",
	"author": {
		"name": "<%= authorName %>",
		"email": "<%= authorEmail %>",
		"url": "https://github.com/<%= authorName %>/<%= projectName %>"
	},
	"bugs": {
		"url": "https://github.com/<%= authorName %>/<%= projectName %>/issues"
	},
	"homepage": "https://github.com/<%= authorName %>/<%= projectName %>#readme",
	"scripts": {
		"start": "set NODE_ENV=development & gulp serve --development",
		"build": "set NODE_ENV=production & gulp build --production",
		"build:components": "set NODE_ENV=production & gulp component --production",
		"clean:node": "rimraf node_modules",
		"update:package": "ncu && ncu -u && npm i"
	},
	"dependencies": {
		"eva-icons": "^1.1.2",
		"swiper": "^5.2.0"
	},
	"devDependencies": {
		"@babel/core": "^7.7.0",
		"@babel/plugin-proposal-class-properties": "^7.7.0",
		"@babel/preset-env": "^7.7.1",
		"@babel/register": "^7.7.0",
		"autoprefixer": "^9.7.1",
		"babel-eslint": "^10.0.3",
		"babel-plugin-add-module-exports": "^1.0.2",
		"babelify": "^10.0.0",
		"browser-sync": "^2.26.7",
		"browserify": "^16.5.0",
		"chai": "^4.2.0",
		"colors": "^1.4.0",
		"css-declaration-sorter": "^4.0.1",
		"del": "^5.1.0",
		"envify": "^4.1.0",
		"eslint": "^6.6.0",
		"fibers": "^4.0.2",
		"glob": "^7.1.5",
		"glob-exec": "^0.1.1",
		"gulp": "^4.0.2",
		"gulp-babel": "^8.0.0",
		"gulp-cached": "^1.1.1",
		"gulp-concat": "^2.6.1",
		"gulp-cssnano": "^2.1.3",
		"gulp-data": "^1.3.1",
		"gulp-debug": "^4.0.0",
		"gulp-eslint": "^6.0.0",
		"gulp-group-css-media-queries": "^1.2.2",
		"gulp-header": "^2.0.9",
		"gulp-if": "^3.0.0",
		"gulp-imagemin": "^6.1.1",
		"gulp-inject": "^5.0.4",
		"gulp-load-plugins": "^2.0.1",
		"gulp-merge-json": "^1.3.1",
		"gulp-notify": "^3.2.0",
		"gulp-plumber": "^1.2.1",
		"gulp-postcss": "^8.0.0",<% if (htmlOption === 'pug') { %>
		"gulp-pug": "^4.0.1",<% } %>
		"gulp-rename": "^1.4.0",
		"gulp-rev": "^9.0.0",
		"gulp-sass": "^4.0.2",
		"gulp-sitemap": "^7.6.0",
		"gulp-size": "^3.0.0",
		"gulp-sizereport": "^1.2.1",
		"gulp-sourcemaps": "^2.6.5",
		"gulp-strip-comments": "^2.5.2",
		"gulp-strip-css-comments": "^2.0.0",
		"gulp-svgo": "^2.1.1",
		"gulp-terser": "^1.2.0",
		"gulp-util": "^3.0.8",
		"gulp-zip": "^5.0.1",
		"gulp.spritesmith": "^6.11.0",
		"imagemin-pngquant": "^8.0.0",
		"lodash": "^4.17.15",
		"merge-stream": "^2.0.0",
		"minimist": "^1.2.0",
		"mkdirp": "^0.5.1",
		"mocha": "^6.2.2",
		"ncu": "^0.2.1",
		"pug-lint": "^2.6.0",
		"rimraf": "^3.0.0",
		"stylelint": "^11.1.1",
		"vinyl-buffer": "^1.0.1",
		"vinyl-source-stream": "^2.0.0",
		"watchify": "^3.11.1"<% if (testFramework !== 'none') { %>,
		"karma": "^4.4.1",
		"karma-browserify": "^6.1.0",
		"karma-chrome-launcher": "~3.0.0", <% if (testFramework === 'jasmine') {%>
		"karma-jasmine": "~2.0.1",
		"jasmine-core": "3.4.0",<% } else if (testFramework === 'mocha') {%>
		"karma-chai": "^0.1.0",
		"karma-mocha": "^1.3.0",
		"mocha": "^6.2.2",
		"chai": "^4.2.0" <% } %> <% } %>
	},
	"keywords": [
		"nodejs",
		"gulp4",
		"pug",
		"sass",
		"es5",
		"es6",
		"babel",
		"html",
		"css",
		"webpack"
	],
	"license": "MIT",
	"componentOptions": {
		"test": <% if (testFramework === 'none') { %> false <% } else { %> true <% } %>,
		"htmlOptions": <% if (htmlOption === 'pug') { %>"pug"<% }  else { %> "" <% } %>,
		"cssOptions": <% if (cssOption === 'sass') { %>"sass"<% }  else { %> "" <% } %>,
		"sassSyntax": <% if (cssOption === 'sass' && sassSyntax === 'sass') { %>"sass"<% }  else if(cssOption === 'sass' && sassSyntax === 'scss') { %> "scss" <% } %>
	}
}
